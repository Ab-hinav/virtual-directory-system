{
  "name": "virtual-directory-system",
  "version": "1.0.0",
  "description": "",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc -p . && tsc-alias",
    "clean": "rm -rf dist .npm-cache",
    "start": "node dist/index.js",
    "test": "NODE_ENV=test NODE_OPTIONS=--experimental-vm-modules jest --coverage",
    "test:watch": "jest --watch",
    "migrate:make": "node --loader ts-node/esm ./node_modules/.bin/knex --knexfile knexfile.ts migrate:make $npm_config_name",
    "migrate:latest": "node --loader ts-node/esm ./node_modules/.bin/knex --knexfile knexfile.ts migrate:latest",
    "migrate:rollback": "node --loader ts-node/esm ./node_modules/.bin/knex --knexfile knexfile.ts migrate:rollback",
    "seed:run": "node --loader ts-node/esm ./node_modules/.bin/knex --knexfile knexfile.ts seed:run"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Ab-hinav/virtual-directory-system.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "type": "module",
  "bugs": {
    "url": "https://github.com/Ab-hinav/virtual-directory-system/issues"
  },
  "homepage": "https://github.com/Ab-hinav/virtual-directory-system#readme",
  "dependencies": {
    "dotenv": "^17.2.2",
    "knex": "^3.1.0",
    "sqlite3": "^5.1.7",
    "tsc-alias": "^1.8.16",
    "zod": "^4.1.5"
  },
  "devDependencies": {
    "@types/jest": "^30.0.0",
    "@types/node": "^24.3.1",
    "@typescript-eslint/eslint-plugin": "^8.42.0",
    "@typescript-eslint/parser": "^8.42.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.4.1",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "tsx": "^4.20.5",
    "typescript": "^5.9.2"
  }
}
